{
  "architecture": {
    "devonProject": false,
    "packages": {
      "pattern": "([a-zA-Z0-9_]+\\.)+(persistence|core|service|gui+)\\.(api|base|impl)(\\.[a-zA-Z0-9_]+)*",
      "groups": [ "root", "layer", "component", "scope", "detail" ],
      "mappings": {  "persistence": "dataaccess", "core": "logic", "gui": "client"}
    }
  }
}